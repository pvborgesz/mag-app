{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "compilerOptions": {
    "module": "es2022",
    "target": "es2017"
  },
  "scripts": {
    "start": "yarn build && node -r dotenv/config dist/main/server.js",
    "build": "rimraf dist && babel src --extensions \".js,.ts\" --ignore \"src/**/*.spec.ts\",\"src/**/*.test.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "dev:queue": "tsnd --clear -r dotenv/config -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules ./src/main/main-queue.ts",
    "dev:server": "tsnd --clear -r dotenv/config -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules ./src/main/server.ts",
    "dev": "tsnd --clear -r dotenv/config -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules ./src/main/server.ts",
    "test": "NODE_ENV=test jest --passWithNoTests --runInBand --max-old-space-size=4096",
    "test:synchronous": "NODE_ENV=test jest --passWithNoTests --max-old-space-size=4096",
    "test:async": "npm run test -- -c jest-asynchronous.config.js",
    "test:sync": "npm run test:synchronous -- -c jest-synchronous.config.js",
    "test:all": "yarn test:sync && yarn test:async --coverage",
    "test:unit": "npm run test -- -c jest-unit.config.js --watch",
    "test:integration": "npm run test -- -c jest-integration.config.js --watch",
    "test:coverage": "npm run test -- --coverage",
    "test:database": "NODE_ENV=test npm run test",
    "test:unit:all": "npm run test -- -c jest-unit.config.js",
    "test:routes": "npm run test -- -c jest-integration.config.js",
    "coverage": "yarn test:coverage",
    "prepare": "husky install",
    "typeorm": "tsnd -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:ds": "yarn typeorm -d src/infra/database/config/postgres/typeorm/pg-data-source.ts",
    "typeorm:jt": "yarn typeorm -d src/infra/database/config/postgres/typeorm/pg-data-source.ts",
    "typeorm:prod": "./node_modules/.bin/typeorm -d dist/infra/database/config/postgres/typeorm/pg-data-source.js",
    "eslint:fix": "tsc --noEmit && eslint --fix './src/**' --ext .ts",
    "m:run": "yarn typeorm:ds migration:run",
    "m:jt:run": "yarn typeorm:jt migration:run",
    "m:jt:revert": "yarn typeorm:jt migration:revert",
    "m:revert": "yarn typeorm:ds migration:revert",
    "m:show": "yarn typeorm:ds migration:show",
    "m:create": "yarn typeorm migration:create",
    "m:pr": "yarn typeorm:prod migration:run",
    "m:ps": "yarn typeorm:prod migration:show"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.0",
    "@babel/node": "^7.20.7",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@jest-mock/express": "^2.0.1",
    "@types/bcrypt": "^5.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.14",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.2.4",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/mapbox__mapbox-sdk": "^0.13.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.12.12",
    "@types/papaparse": "^5.3.7",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.7.10",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "eslint": "^9.8.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "jest-mock-extended": "^3.0.1",
    "lint-staged": "^13.1.0",
    "papaparse": "^5.4.1",
    "prettier": "^2.8.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.3",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.1",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@bull-board/api": "^5.9.1",
    "@bull-board/express": "^5.9.1",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "bull": "^4.11.5",
    "class-transformer": "^0.5.1",
    "date-fns": "^2.29.3",
    "date-fns-tz": "^2.0.0",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.1.2",
    "validator": "^13.7.0",
    "xmldom": "^0.6.0"
  }
}